<body style="padding-bottom:30px">
	<div class="container-sm">

  <!-- ================= Data ================== -->

  <div class="container-sm style-is-cool">
  <h1><%= name %></h1>
  <div class="row row-cols-1 row-cols-md-3 g-4">
    <div class="col">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <h5 class="card-title">Users</h5>
          <p class="card-text"><%= users %></p>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <h5 class="card-title">Roles</h5>
          <p class="card-text"><%= roles %></p>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <h5 class="card-title">Channels</h5>
          <p class="card-text"><%= channels %></p>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <h5 class="card-title">Messages</h5>
          <p class="card-text"><%= msg %></p>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <h5 class="card-title">Messages Deleted</h5>
          <p class="card-text"><%= msgdel %></p>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <h5 class="card-title">Messages Edited</h5>
          <p class="card-text"><%= msgedit %></p>
        </div>
      </div>
    </div>
  </div>
    <div class="col">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <div id="channel" style="height: 300px; width: 100%;"></div>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <div id="emoji" style="height: 300px; width: 100%;"></div>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <div id="member" style="height: 300px; width: 100%;"></div>
        </div>
      </div>
    </div>
  </div>

</div>

<script>
	fetch('https://serversmp.xyz/assets/json/navbar.json')
		.then(function (response) {
			return response.json();
		})
		.then(function (data) {
			appendData(data);
		})
		.catch(function (err) {
			console.log('error: ' + err);
		});

	function appendData(data) {
		document.getElementById("nav-bar").innerHTML = data.navbar;
	}
</script>

<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
      google.charts.load('current', {'packages':['bar']});
      google.charts.setOnLoadCallback(drawChart);

      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['Month', 'Channels Created', 'Channels Deleted', 'Channels Pin Update', 'Channels Update'],
          ['January', Number(<%= cc.january.length %>), Number(<%= cd.january.length %>), Number(<%= cpu.january.length %>), Number(<%= cu.january.length %>)],
