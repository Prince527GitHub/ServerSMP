          ['April', Number(<%= cc.april.length %>), Number(<%= cd.april.length %>), Number(<%= cpu.april.length %>), Number(<%= cu.april.length %>)],
          ['May', Number(<%= cc.may.length %>), Number(<%= cd.may.length %>), Number(<%= cpu.may.length %>), Number(<%= cu.may.length %>)],
          ['June', Number(<%= cc.june.length %>), Number(<%= cd.june.length %>), Number(<%= cpu.june.length %>), Number(<%= cu.june.length %>)],
          ['July', Number(<%= cc.july.length %>), Number(<%= cd.july.length %>), Number(<%= cpu.july.length %>), Number(<%= cu.july.length %>)],
          ['August', Number(<%= cc.august.length %>), Number(<%= cd.august.length %>), Number(<%= cpu.august.length %>), Number(<%= cu.august.length %>)],
          ['September', Number(<%= cc.september.length %>), Number(<%= cd.september.length %>), Number(<%= cpu.september.length %>), Number(<%= cu.september.length %>)],
          ['October', Number(<%= cc.october.length %>), Number(<%= cd.october.length %>), Number(<%= cpu.october.length %>), Number(<%= cu.october.length %>)],
          ['November', Number(<%= cc.november.length %>), Number(<%= cd.november.length %>), Number(<%= cpu.november.length %>), Number(<%= cu.november.length %>)],
          ['December', Number(<%= cc.december.length %>), Number(<%= cd.december.length %>), Number(<%= cpu.december.length %>), Number(<%= cu.december.length %>)],
        ]);

        var options = {
          chart: {
            title: 'Channel Data',
            subtitle: '<%= name %>',
          },
          hAxis: {
            titleTextStyle: {color: '#607d8b'}, 
            gridlines: { count:0}, 
            textStyle: { color: '#b0bec5', fontName: 'Concert One', fontSize: '12', bold: true}
          },
          vAxis: {
            minValue: 0, 
            gridlines: {color:'#37474f', count:4}, 
            baselineColor: 'transparent'
          },
          legend: {position: 'top', alignment: 'center', textStyle: {color:'#607d8b', fontName: 'Concert One', fontSize: '12'} },
          colors: ["#3f51b5","#2196f3","#03a9f4","#00bcd4","#009688","#4caf50","#8bc34a","#cddc39"],
          areaOpacity: 0.24,
          lineWidth: 1,
          backgroundColor: 'transparent',
          chartArea: {
            backgroundColor: "transparent",
            width: '100%',
            height: '80%'
          },
          pieSliceBorderColor: '#263238',
          pieSliceTextStyle:  {color:'#607d8b' },
          pieHole: 0.9,
          bar: {groupWidth: "40" },
          colorAxis: {colors: ["#3f51b5","#2196f3","#03a9f4","#00bcd4"] },
          backgroundColor: 'transparent',
          datalessRegionColor: '#37474f',
          displayMode: 'regions'
        };

        var chart = new google.charts.Bar(document.getElementById('channel'));

        chart.draw(data, google.charts.Bar.convertOptions(options));
      }
</script>
