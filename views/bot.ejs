<body style="padding-bottom:30px">
	<div class="container-sm">

		<!-- ================= Name & logo ================== -->

		<h1>ServerSMP - BOT</h1>
		<img src="https://serversmp.xyz/web/bot/assets/image/icon.png" alt="ServerSMP - BOT" width="180" height="180" ></img>

	<!-- ================= Description of ServerSMP - API ================== -->

	<h4 style="color:#6c8285;">A bot for all your needs! May it be moderation, info on things from your server, or even random pictures of ducks. We have it all!</h4>

  <!-- ================= APIs ================== -->

  <div class="container-sm style-is-cool">
  <div class="row">
    <div class="col-sm-6">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <h5 class="card-title">Servers</h5>
          <p class="card-text"><%= servers %></p>
        </div>
      </div>
    </div>
    <div class="col-sm-6">
      <div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
        <div class="card-body">
          <h5 class="card-title">Users</h5>
          <p class="card-text"><%= users %></p>
        </div>
      </div>
    </div>
    <div class="col-sm-6">
		<div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
		  <div class="card-body">
			<h5 class="card-title">Commands</h5>
			<p class="card-text">List of commands.</p>
			<a href="https://serversmp.xyz/web/bot/commands.html" class="btn btn-primary">Link</a>
		  </div>
		</div>
	  </div>
	  <div class="col-sm-6">
		<div class="card text-white bg-dark mb-3"class="card text-white bg-dark mb-3">
		  <div class="card-body">
			<h5 class="card-title">Invite</h5>
			<p class="card-text">Invite the bot.</p>
			<a href="https://discord.com/api/oauth2/authorize?client_id=778409873573412874&scope=applications.commands+bot&permissions=1073203574" class="btn btn-primary">Link</a>
		  </div>
		</div>
	  </div>
  </div>
  </div>

</div>

<script>
	fetch('https://serversmp.xyz/assets/json/navbar.json')
		.then(function (response) {
			return response.json();
		})
		.then(function (data) {
			appendData(data);
		})
		.catch(function (err) {
			console.log('error: ' + err);
		});

	function appendData(data) {
		document.getElementById("nav-bar").innerHTML = data.navbar;
	}
</script>

</body>
</html>